{
	"name": "Chinese Overlay",
	"version": "0.0.1",
	"description": "Highlights HSK vocabulary on Chinese language websites",
	"icons": {},
	"background": {
		"persistent": false,
		"scripts": ["bg.html"]
	},
	"content_scripts": [{
		"all_frames": true,
		"css": ["css/content.css"],
		"js": [
			"js/lib/jquery-migrate-1.4.1.min.js",
			"js/main.js"
		],
		"matches": ["<all_urls>"],
		"run_at": "document_start"
	}],
	"permissions": [
		"contentSettings",
		"<all_urls>"
	],
	"manifest_version": 2
}